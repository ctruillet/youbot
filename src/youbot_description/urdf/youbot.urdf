						<mean>0.00</mean>
						<stddev>0.01</stddev>
					</noise>
				</ray>
				<update_rate>10</update_rate>
				<pose frame=''>0.3 0 -0.03 0 -0 0</pose>
        <plugin name='gazebo_ros_base_laser_front_controller'
			          filename='libgazebo_ros_ray_sensor.so'>
